{
  "name": "kengu-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend": "cd ../ && python -m services.serve",
    "start": "concurrently \"npm run backend\" \"npm run serve -- --host 0.0.0.0\""
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chromedriver": "^142.0.1",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "debug": "^4.4.3",
    "element-plus": "^2.11.7",
    "express": "^5.1.0",
    "pinia": "^3.0.4",
    "selenium-webdriver": "^4.38.0",
    "vue": "^3.2.13",
    "vue-router": "^4.6.3"
  },
  "optionalDependencies": {
    "dompurify": "^3.3.0",
    "marked": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
